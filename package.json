{
  "name": "bookit",
  "version": "1.0.0",
  "description": "Website for university books",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon index.js --ignore client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Alexander, August, Theis, Jonas",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.0.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "react-router-dom": "^6.11.1",
    "styled-components": "^5.3.10"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonasSkjodt/bookit.git"
  },
  "keywords": [
    "webdeveloping",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/JonasSkjodt/bookit/issues"
  },
  "homepage": "https://github.com/JonasSkjodt/bookit#readme"
}
